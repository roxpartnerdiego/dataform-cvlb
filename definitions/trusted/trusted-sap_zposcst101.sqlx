      dt_inclusao,
      hr_inclusao,
      nfe01,
      versao01,
      fg_bloqlote,
      cliente_fidel,
      recordstamp
    FROM
        `RAW.sap_zposcst101` A
    WHERE
      PARTITIONTIME=(
      SELECT
        MAX(PARTITIONTIME)
      FROM
        `RAW.sap_zposcst101`))
  SELECT
    *
  FROM
    TABELA) S
ON
  T.werks = S.werks
  WHEN MATCHED THEN UPDATE SET 
  T.mandt = S.mandt,
  T.werks = S.werks,
  T.cdsis = S.cdsis,
  T.nupdv = S.nupdv,
  T.tpopr = S.tpopr,
