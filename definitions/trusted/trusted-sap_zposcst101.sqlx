MERGE
  `TRUSTED.sap_zposcst101` T
USING
  (
  WITH
    TABELA AS (
    SELECT
      DISTINCT werks,
      mandt,
      cdsis,
      nupdv,
      tpopr,
      mvend,
      dtopr,
      ntran,
      nlote,
      cod_operador01,
      nome_operador01,
