TRUNCATE TABLE REFINED.ISSUE;

INSERT INTO 
    REFINED.ISSUE (
SELECT
  id,
  name,
  "CEV" AS BANDEIRA,
  CURRENT_TIMESTAMP() AS DATA_ATUALIZACAO
FROM `TRUSTED.oms_issue_cev`

UNION ALL

SELECT
  id,
  name,
  "LB" AS BANDEIRA,
  CURRENT_TIMESTAMP() AS DATA_ATUALIZACAO
FROM `TRUSTED.oms_issue_lb` )